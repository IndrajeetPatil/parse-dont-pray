      "source": [
        "from pydantic import BaseModel, EmailStr, Field\n",
        "\n",
        "# Concise and clear\n",
        "class User(BaseModel):\n",
        "    username: str = Field(min_length=3)\n",
        "    email: EmailStr\n",
        "    age: int = Field(ge=0)\n",
        "\n",
        "# That's it! Full validation, type safety, IDE support\n",
        "user = User(**data)  # â† One line replaces 50+ lines"
      ],
